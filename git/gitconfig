[alias]
	br = branch
	ci = commit
	co = checkout
	df = diff
	ds = diff --staged
	g = grep -I
	lg = log -p
	pb = publish-branch
	pp = !"echo 'Pull'; git pull; echo 'Push'; git push"
	rb = rbranch
	rc = rank-contributors
	rv = review
	sm = show-merges
	st = status
	amend = commit --amend -C HEAD
	undo = reset --soft HEAD^
	ksdiff = difftool -y -t Kaleidoscope
[color]
	 ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	log = true
[difftool "Kaleidoscope"]
	cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[core]
	excludesfile = /Users/petersendidit/.gitignore
[help]
	autocorrect = 1
